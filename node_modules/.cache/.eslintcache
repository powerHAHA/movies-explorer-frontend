{});\n\tconst [isValid, setIsValid] = useState(false);\n\n\tconst handleChange = (event) => {\n\t\tconst target = event.target;\n\t\tconst name = target.name;\n\t\tconst value = target.value;\n\t\tsetValues({ ...values, [name]: value });\n\t\tsetErrors({ ...errors, [name]: target.validationMessage });\n\t\tsetIsValid(target.closest(\"form\").checkValidity());\n\t};\n\n\tconst resetForm = useCallback(\n\t\t(newValues = {}, newErrors = {}, newIsValid = false) => {\n\t\t\tsetValues(newValues);\n\t\t\tsetErrors(newErrors);\n\t\t\tsetIsValid(newIsValid);\n\t\t},\n\t\t[setValues, setErrors, setIsValid]\n\t);\n\n\treturn { values, setValues, handleChange, errors, isValid, resetForm };\n}\n\n\n\n\n\n","/Users/denis/dev/movies-explorer-frontend/src/components/NavTab/NavTab.jsx",[],[],"/Users/denis/dev/movies-explorer-frontend/src/components/AboutProject/AboutProject.jsx",[],[],"/Users/denis/dev/movies-explorer-frontend/src/components/Techs/Techs.jsx",[],[],"/Users/denis/dev/movies-explorer-frontend/src/components/AboutMe/AboutMe.jsx",[],[],"/Users/denis/dev/movies-explorer-frontend/src/components/Promo/Promo.jsx",["184"],[],"import React from 'react';\nimport './Promo.css';\nimport logoPromo from '../../images/logo-promo.svg';\n\nexport const Promo = () => {\n\treturn (\n\n\t\t<section className=\"promo\">\n\t\t\t<h1 className='promo__title'>Учебный проект студента факультета Веб-разработки.</h1>\n\t\t</section>\n\n\t)\n}","/Users/denis/dev/movies-explorer-frontend/src/components/SearchForm/SearchForm.jsx",["185"],[],"import React, { useState, useEffect } from 'react';\nimport './SearchForm.css';\nimport { FilterCheckbox } from '../FilterCheckbox/FilterCheckbox';\nimport { useLocation } from 'react-router-dom';\n\nexport const SearchForm = ({ onSearch, filterCheckbox, onError }) => {\n\tconst [error, setError] = useState('');\n\tconst { pathname } = useLocation();\n\n\tconst [shortFilms, setShortFilms] = useState(false);\n\tconst [movieName, setMovieName] = useState('');\n\t